<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>

<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="main.java.MainMenuController"
          stylesheets="@css/stylesheet.css">

    <Label text="Color Matcher" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.halignment="CENTER"/>
    <Button text="Match Colors" GridPane.columnIndex="0" GridPane.rowIndex="1" onAction="#loadPosterizer"/>
    <Button text="Quit" GridPane.columnIndex="0" GridPane.rowIndex="2" onAction="#quit"/>
</GridPane>
